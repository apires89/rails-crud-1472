<%= form_with model: @restaurant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <%= f.label :rating %>
  <%= f.number_field :rating %>
  <%= f.submit "Push it!" %>
<% end %>
